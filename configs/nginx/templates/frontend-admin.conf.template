# Frontend admin server
server {
    listen 443 ssl;
    listen [::]:443 ssl;
    server_name ${FRONTEND_ADMIN_SERVER_NAME};

    ssl_certificate $ssl_certificate;
    ssl_certificate_key $ssl_certificate_key;

    charset utf-8; 
    
    location / {
        root /usr/share/nginx/html;
        index index.html;
        # proxy_pass http://frontend-admin:3000;
    }
}
